import numpy as np
# Terdapat list a dan b
a = [1,2,3]
b = [4,5,6]

# Jumlahkan
hasil = a + b

print(hasil) # [1,2,3,4,5,6]

==================================================================================================================================================================

def add(list a,list b):
  result = []
  for first, second in zip(list a, list b):
    result.append(first + second)
    return result

